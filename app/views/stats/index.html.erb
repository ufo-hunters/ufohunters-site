    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&libraries=visualization"></script>
    <script>
// Adding 1000 Data Points
var map, pointarray, heatmapLayer;

function initialize() {
  var mapOptions = {
    zoom: 2,
    center: new google.maps.LatLng(34.16473667601063, -40.57035354999999),
    mapTypeId: google.maps.MapTypeId.SATELLITE
  };

  map = new google.maps.Map(document.getElementById('map-canvas'),
      mapOptions);

  heatmapLayer = new google.maps.FusionTablesLayer({
    query: {
      select: 'latitude, longitude',
      from: '1dsJO2_SxD3ypYepYBxHKfpofkEVVq5eU3E7qvOrv'
    },
    heatmap: {
      enabled: true
    }
  });

  heatmapLayer.setMap(map);
  heatmapLayer.set('radius', 20);
}

google.maps.event.addDomListener(window, 'load', initialize);

    </script>

    <div class="row">
		  <div class="span12">
		    <div id="map-canvas" style="width: 100%; height: 600px;"></div>
		  </div>
		</div>
